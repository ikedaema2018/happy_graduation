<p id="notice"><%= notice %></p>

<h1>Community Tag Lists</h1>

<table>
  <thead>
    <tr>
      <th>Tag name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_tag_lists.each do |community_tag_list| %>
      <tr>
        <td><%= community_tag_list.tag_name %></td>
        <td><%= link_to 'Show', community_tag_list %></td>
        <td><%= link_to 'Edit', edit_community_tag_list_path(community_tag_list) %></td>
        <td><%= link_to 'Destroy', community_tag_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community Tag List', new_community_tag_list_path %>
